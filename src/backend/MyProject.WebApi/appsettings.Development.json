{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "Database": "Host=localhost;Port=13004;Database=myproject;Username=admin;Password=pwd"
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    }
  },
  "Cors": {
    "AllowAllOrigins": true,
    "AllowedOrigins": [],
    "PolicyName": "DefaultCorsPolicy"
  }
}
