{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Error",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ConnectionStrings": {
    "Database": "<DB_CONNECTION_STRING>"
  },
  "Caching": {
    "DefaultExpiration": "00:10:00",
    "Redis": {
      "Enabled": false,
      "ConnectionString": "<REDIS_CONNECTION_STRING>",
      "Password": "<REDIS_PASSWORD>",
      "UseSsl": true,
      "DefaultDatabase": 0,
      "InstanceName": "{INIT_PROJECT_SLUG}:",
      "ConnectTimeoutMs": 5000,
      "SyncTimeoutMs": 5000,
      "AsyncTimeoutMs": 5000,
      "AbortOnConnectFail": false,
      "ConnectRetry": 3,
      "KeepAliveSeconds": 60
    },
    "InMemory": {
      "SizeLimit": 1024,
      "ExpirationScanFrequency": "00:01:00",
      "CompactionPercentage": 0.05
    }
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 120,
      "Window": "00:01:00",
      "QueueLimit": 0,
      "QueueProcessingOrder": "OldestFirst"
    },
    "Registration": {
      "PermitLimit": 5,
      "Window": "00:01:00",
      "QueueLimit": 0,
      "QueueProcessingOrder": "OldestFirst"
    },
    "Auth": {
      "PermitLimit": 10,
      "Window": "00:01:00",
      "QueueLimit": 0,
      "QueueProcessingOrder": "OldestFirst"
    },
    "Sensitive": {
      "PermitLimit": 5,
      "Window": "00:05:00",
      "QueueLimit": 0,
      "QueueProcessingOrder": "OldestFirst"
    },
    "AdminMutations": {
      "PermitLimit": 30,
      "Window": "00:01:00",
      "QueueLimit": 0,
      "QueueProcessingOrder": "OldestFirst"
    }
  },
  "Authentication": {
    "Jwt": {
      "Key": "CHANGE_ME_IN_PRODUCTION__This_Is_A_Development_Only_Placeholder_Key_64chars!",
      "Issuer": "MyProjectIssuer",
      "Audience": "MyProjectAudience",
      "ExpiresInMinutes": 10,
      "SecurityStampClaimType": "security_stamp",
      "RefreshToken": {
        "ExpiresInDays": 7
      }
    }
  },
  "JobScheduling": {
    "Enabled": true,
    "WorkerCount": 4
  },
  "Cors": {
    "AllowAllOrigins": false,
    "AllowedOrigins": [
      "https://your-production-domain.com"
    ],
    "PolicyName": "DefaultCorsPolicy"
  },
  "Email": {
    "FromAddress": "noreply@example.com",
    "FromName": "MyProject",
    "FrontendBaseUrl": "https://your-production-domain.com",
    "Smtp": {
      "Host": "",
      "Port": 587,
      "Username": "",
      "Password": "",
      "UseSsl": true
    }
  },
  "AllowedHosts": "*"
}
