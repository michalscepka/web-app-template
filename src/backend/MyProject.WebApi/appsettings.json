{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Error",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ConnectionStrings": {
    "Database": "<DB_CONNECTION_STRING>"
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 120,
      "Window": "00:01:00"
    }
  },
  "Authentication": {
    "Jwt": {
      "Key": "A_Very_Long_Secure_Random_Secret_Key_ChangeThis123!@#4567890",
      "Issuer": "MyProjectIssuer",
      "Audience": "MyProjectAudience",
      "ExpiresInMinutes": 10,
      "SecurityStampClaimType": "security_stamp",
      "RefreshToken": {
        "ExpiresInDays": 7
      }
    }
  },
  "Cors": {
    "AllowAllOrigins": false,
    "AllowedOrigins": [
      "https://your-production-domain.com"
    ],
    "PolicyName": "DefaultCorsPolicy"
  },
  "AllowedHosts": "*"
}
